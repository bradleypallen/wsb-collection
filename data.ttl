wsbc:abr40-e a bf:Classification ;
    rdfs:label "SOUND RECORDINGS" ;
    rdfs:subClassOf wsbc:abr40 ;
    skos:definition "\"... Burroughs’ own sound recordings, and his contributions to or appearances on those by others.\"-Introduction." .

wsbc:abr40-f a bf:Classification ;
